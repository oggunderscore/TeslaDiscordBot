{
  "name": "pchat-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "dev": "npm run watch ./src/bin/www",
    "prod": "npm run build && set NODE_ENV=production node ./dist/bin/www",
    "build": "npm run clean && babel ./src --config-file ./.babelrc --out-dir dist",
    "clean": "rimraf dist",
    "watch": "nodemon --config ./nodemon.json"
  },
  "dependencies": {
    "bigint-hash": "^0.2.2",
    "cookie-parser": "^1.4.4",
    "debug": "~2.6.9",
    "discord.js": "^12.0.1",
    "dotenv": "^8.2.0",
    "express": "~4.16.0",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^9.0.0",
    "morgan": "~1.9.0",
    "npm-run-all": "^4.1.5",
    "openpgp": "^4.10.1",
    "rimraf": "^2.7.1",
    "sequelize": "^5.21.5",
    "socket.io": "^2.3.0",
    "teslajs": "^4.9.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "babel-core": "^7.0.0-bridge.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.20.1",
    "nodemon": "^1.19.4"
  }
}
